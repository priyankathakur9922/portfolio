.skills-bar{width:1000px;
            margin-left:200px;
            background:#fff;
			border-radius:10px;
			padding:25px 30px;
			box-shadow:5px 5px 10px rgba(0,0,0,0.2);}
.skills-bar .bar{margin:20px 0;}
.skills-bar .bar:first-child{margin-top:0px;}
.skills-bar .bar .inf{margin-bottom:5px;}
.skills-bar .bar .inf span{font-size:17px;
                           font-weight:500;}
.skills-bar .bar .progress-line{position:relative;
	                            height:10px;
                                width:100%;
								background:#f0f0f0;
								box-shadow:inset 0px 1px 1px rgba(0,0,0,0.5),
								                 0px 1px rgba(255,255,255,0.8);}
.bar .progress-line span{height:100%;
                         width:80%;
						 background:#6665ee;
						 position:absolute;
						 border-radius:10px;}
.progress-line.html span{ width:"90%";}
.progress-line.css span{ content:"80%";}	
.progress-line.javascript span{ content:"75%";}	
.progress-line.php span{ content:"70%";}	
.progress-line.wordpress span{ content:"85%";}	
.progress-line.photoshop span{ content:"60%";}	
.bar .progress-line span::before{position:absolute;
                                 content:"";
								 right:0;
								 top:-10px;
								 height:0;
								 width:0;
								 border:7px solid transparent;
								 border-bottom-width:0px;
								 border-right-width:0px;
								 border-top-color:#000;}
.bar .progress-line span::after{position:absolute;
                                 content:"80%";
								 right:0;
								 top:-28px;
								 color:#fff;
								 font-size:12px;
								 font-weight:500;
								 background:#000;
								 padding:1px 8px;
								 border-radius:3px;
								 }
.progress-line.html span::after{ content:"90%";}
.progress-line.css span::after{ content:"80%";}	
.progress-line.javascript span::after{ content:"75%";}	
.progress-line.php span::after{ content:"70%";}	
.progress-line.wordpress span::after{ content:"85%";}	
.progress-line.photoshop span::after{ content:"60%";}									 
